<h1>Listing table3s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @table3s.each do |table3| %>
      <tr>
        <td><%= table3.name %></td>
        <td><%= table3.code %></td>
        <td><%= link_to 'Show', table3 %></td>
        <td><%= link_to 'Edit', edit_table3_path(table3) %></td>
        <td><%= link_to 'Destroy', table3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

